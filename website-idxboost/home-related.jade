					img(src="images/resf/pc.png", title="idxBoost", alt="FREE WordPress Websites With IDX")
					img(src="images/resf/cpanel-0.png", title="idxBoost", alt="FREE WordPress Websites With IDX").pc-img.idx-abs-img.left.idx-pc-show
					img(src="images/resf/cpanel-1.png", title="idxBoost", alt="FREE WordPress Websites With IDX").pc-img.idx-pc-show.center
					img(src="images/resf/cpanel-2.png", title="idxBoost", alt="FREE WordPress Websites With IDX").pc-img.idx-abs-img.right.idx-pc-show
