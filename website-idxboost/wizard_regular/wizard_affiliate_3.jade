doctype html
html(lang='en')
  head
    title FlexIdx | Welcome
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no")
    meta(name="apple-mobile-web-app-capable", content="yes")
    meta(name="apple-mobile-web-app-status-bar-style", content="#203965")
    meta(name="theme-color", content="#203965")
    meta(name="msapplication-navbutton-color", content="#203965")
    link(rel="stylesheet", href="../css/main.css")

  body
    main.wrap-idx-content
      header#header
        .idx-content-header
          .idx-wrap-logo
            a(href="/", title="idxBoost").logo
              img(src="../images/logo.png", title="idxBoost", alt="idxBoost")
            button#btn-responsive: span
          #idx-wrap-responsive
            nav.idx-top-nav
              ul
                li: a(href="tel:8778758745", title="(877) 875-8745") (877) 875-8745
                li: a(href="#", title="News") News
                li: a(href="#", title="Support") Support
                li: a(href="#", title="Login") Login
                li: a(href="#", title="Register") Register
            nav.idx-main-menu
              ul
                li: a(href="#", title="Wordpress IDX Features") Wordpress IDX Features
                li: a(href="#", title="FREE IDX Websites") FREE IDX Websites
                li: a(href="#", title="Partners") Partners
                li: a(href="#", title="Live Demo") Live Demo
                li.idx-btn-content
                  button.idx-btn-site
                    span Start Now
      .r-overlay
      section.idx-site-content
        h1.idx-title Payment Summary
        
        form(action="wizard_affiliate_4.html", method="post")
          .idx-box-content
            .idx-product-information
              ul.idx-pi-header
                li Product
                li Unit Price
                li Subtotal
              ul.idx-pi-body
                li
                  .idx-pi-item
                    .idx-flex-mbox
                      img(src="../images/resf.jpg",  alt="RESF Exclusive")
                      .idx-product
                        h2 RESF Agent Theme
                        span Exclusive RESF Agent Theme
                        span.idx-free Free Setup      
                  .idx-pi-item.t-head(data-text="Unit Price") FREE
                  .idx-pi-item.t-head(data-text="Subtotal") FREE
                li.no-line
                  .idx-pi-item
                    .idx-product
                      h2 IDX Monthly Free
                        span.info-bubble ?
                          span.bubble-box
                            span.bubble-info Lorem Ipsum
                      span Exclusive RESF Agent Theme  
                  .idx-pi-item.t-head(data-text="Unit Price") $59/mo
                  .idx-pi-item.t-head(data-text="Subtotal") $59.00
                li
                  ul.list-options
                    li.opt-content
                      input(type="radio", value="1", id="option1",name="radio")
                      label(for="option1") Monthly
                      |$59/mo
                    li.opt-content
                      input(type="radio", value="2", id="option2",name="radio")
                      label(for="option2") 6 Months
                      |Save 10%
                    li.opt-content.special-bold
                      input(type="radio", value="3", id="option3",name="radio", checked="checked")
                      label(for="option3") 1 Year
                      span $Save/15%
                li
                  .idx-pi-item
                    .idx-product
                      h2.opt-content
                        input(type="checkbox", value="4", id="option-1",name="radio-1")
                        label(for="option-1") Hosting
                        span.info-bubble ?
                          span.bubble-box
                            span.bubble-info Lorem Ipsum
                      span Have idxBoost host your website     
                  .idx-pi-item.t-head(data-text="Unit Price") $9/mo
                  .idx-pi-item.t-head(data-text="Subtotal") FREE
                li
                  .idx-pi-item
                    .idx-product
                      h2 MLS PASS-Though Access Fee
                        span.info-bubble ?
                          span.bubble-box
                            span.bubble-info Lorem Ipsum
                      span Miami Associations of REALTORS© (MAOR) - Miami, FL   
                  .idx-pi-item.t-head(data-text="Unit Price") $5/mo
                  .idx-pi-item.t-head(data-text="Subtotal") $5.00
            
            .idx-payment-information
              ul
                li.item-money
                  h3 One-Time 
                    span Setup Free
                  span.price $0
                    label 00
                li.item-money
                  h3 Monthly Total 
                    span MLS Feed + Hosting
                  span.price $64
                    label 00
                li.item-center
                  button.idx-link-add(data-text="Remove promo code")
                    span Add a promo code
                  .idx-promo-cod
                    input(type="text")#idx-input-code
                  button.idx-btn-site.icon-padlock
                    span Procced To Checkout
                  .idx-security-label Safe & Secure Payment
                  label.idx-min-description The setup fee is non-refundable and covers account creation, IDX license processing, and initial training and support.
                  .idx-chat-btn
                    h3 Questions?
                    button.idx-btn-default.icon-chat
                      span Chat Now
